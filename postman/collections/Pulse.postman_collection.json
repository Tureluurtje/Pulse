          "event": [
            {
              "listen": "test",
              "script": {
                "id": "2e3bdf60-b8e6-4303-98ea-f7a427e6fb0d",
                "exec": [
                  "// Visualization Script",
                  "pm.test(\"Status code is 200\", function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "// Visualization Script",
                  "const template = `",
                  "  <h3>Token Lengths</h3>",
                  "  <table>",
                  "    <tr><th>Token</th><th>Length</th></tr>",
                  "    <tr><td>Access Token</td><td>{{accessTokenLength}}</td></tr>",
                  "    <tr><td>Refresh Token</td><td>{{refreshTokenLength}}</td></tr>",
                  "  </table>",
                  "  <div style=\"max-width:400px;\">",
                  "    <canvas id=\"barChart\" height=\"100\"></canvas>",
                  "  </div>",
                  "  <script src=\"https://cdn.jsdelivr.net/npm/chart.js@2.9.4/dist/Chart.min.js\"></script>",
                  "  <script>",
                  "    pm.getData(function (err, value) {",
                  "      var ctx = document.getElementById('barChart').getContext('2d');",
                  "      new Chart(ctx, {",
                  "        type: 'bar',",
                  "        data: {",
                  "          labels: ['Access Token', 'Refresh Token'],",
                  "          datasets: [{",
                  "            label: 'Token Length',",
                  "            data: [value.accessTokenLength, value.refreshTokenLength],",
                  "            backgroundColor: ['#36a2eb', '#ff6384']",
                  "          }]",
                  "        },",
                  "        options: {",
                  "          legend: { display: false },",
                  "          title: { display: false },",
                  "          scales: {",
                  "            yAxes: [{",
                  "              ticks: { beginAtZero: true }",
                  "            }]",
                  "          }",
                  "        }",
                  "      });",
                  "    });",
                  "  <\\/script>",
                  "`;",
                  "",
                  "function createPayload() {",
                  "  var json = pm.response.json();",
                  "  return {",
                  "    accessTokenLength: json.access_token ? json.access_token.length : 0,",
                  "    refreshTokenLength: json.refresh_token ? json.refresh_token.length : 0",
                  "  };",
                  "}",
                  "",
                  "pm.visualizer.set(template, createPayload());"
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            },
